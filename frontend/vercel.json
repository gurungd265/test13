{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://calmarket-env-1.eba-tbq9rmtf.us-east-1.elasticbeanstalk.com/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
